  <DataSets>
    <DataSet Name="DataSet1">
      <Fields>
        <Field Name="ServerName">
          <DataField>ServerName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BackupDate">
          <DataField>BackupDate</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="BackupTime">
          <DataField>BackupTime</DataField>
          <rd:TypeName>System.Int32</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>eMDW</DataSourceName>
        <CommandType>Text</CommandType>
        <CommandText>SELECT s.ServerName, convert(varchar(10),b.BackupStartDate, 101) AS BackupDate 
       ,SUM(DATEDIFF(minute,b.BackupStartDate, b.BackupEndDate)) AS BackupTime
FROM [eMDW].[dbo].[DB_BackupData_Archive] b join DB_ServerMain s
  ON b.DBServerID = s.DBServerID
Where b.Status = 'Compliant'  AND b.BackupType = 'Full'
Group by S.ServerName, convert(varchar(10),b.BackupStartDate, 101)
order by s.ServerName, BackupDate</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
        <Timeout>0</Timeout>
      </Query>
    </DataSet>
  </DataSets>
