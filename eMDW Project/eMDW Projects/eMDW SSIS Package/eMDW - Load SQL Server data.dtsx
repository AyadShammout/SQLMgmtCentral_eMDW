                          UITypeEditor="Microsoft.DataTransformationServices.Controls.ModalMultilineStringEditor, Microsoft.DataTransformationServices.Controls, Version=11.0.0.0, Culture=neutral, PublicKeyToken=89845dcd8080cc91">--Select only Online DBs and not joined in AlwaysOn AG (accessible db)
Select CAST(SERVERPROPERTY ('ServerName') AS    NVARCHAR(128)) as ServerName, name AS DBName,      Database_id , state_desc
from master.sys.databases 
Where Name not in (
select c.database_name 
from sys.dm_hadr_availability_group_states s join sys.availability_databases_cluster c 
on s.group_id = c.group_id
Where s.primary_recovery_health is NULL)
AND state_desc = 'Online'
order by ServerName,    name</property>
