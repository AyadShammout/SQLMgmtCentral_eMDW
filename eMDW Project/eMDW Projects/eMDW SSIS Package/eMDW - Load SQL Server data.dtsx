DECLARE @Tcp_Port nvarchar(1024), @str varchar(4000), @instance varchar(50)
DECLARE @sqlser varchar(50), @str2 varchar(4000)
DECLARE @cpu int, @ht int, @mem_mb int, @sql_starttime  smalldatetime

Set @instance = Cast(SERVERPROPERTY('InstanceName') as varchar(50))
If @instance IS NOT NULL
Begin
   Set @str = 'SOFTWARE\Microsoft\\Microsoft SQL Server\'+@instance+'\MSSQLServer\SuperSocketNetLib\Tcp'
   EXEC master.dbo.xp_instance_regread N'HKEY_LOCAL_MACHINE',
                                        @str,
                                        N'TcpPort',
                                        @Tcp_Port OUTPUT

   Set @str2 = 'SYSTEM\CurrentControlSet\Services\MSSQL$'+@instance
   EXEC master..xp_instance_regread N'HKEY_LOCAL_MACHINE',
								@str2,
								'objectname', 
								@sqlser OUTPUT 

  End                                          
Else      
Begin                                  
  Set @Tcp_Port = 1433
  
  EXEC master..xp_instance_regread N'HKEY_LOCAL_MACHINE',
								'SYSTEM\CurrentControlSet\Services\MSSQLSERVER',
								'objectname', 
								@sqlser OUTPUT 
End								


Select @cpu = cpu_count, @mem_mb = ((physical_memory_in_bytes/1024)/1024), @ht = hyperthread_ratio, @sql_starttime = (Select create_date from sys.databases Where name = 'tempdb')--sqlserver_start_time 
From sys.dm_os_sys_info


SELECT
@@MICROSOFTVERSION/POWER(2,24) AS SQLVersion,
CAST(SERVERPROPERTY('ProductVersion') AS NVARCHAR(128)) AS ProductVersion,
CAST(SERVERPROPERTY('ProductLevel') AS NVARCHAR(50)) AS ProductLevel,
CAST(SERVERPROPERTY('Edition') AS NVARCHAR(128)) AS Edition,

Case CAST(SERVERPROPERTY('EngineEdition') AS VARCHAR(50))
 When 1 Then 'Personal' 
 When 2 Then 'Standard'
 When 3 Then 'Enterprise'
 When 4 Then 'Express'
End AS EngineEdition,
CAST(SERVERPROPERTY('ComputerNamePhysicalNetBIOS') AS NVARCHAR(128))AS ComputerNetBIOS,
CAST(SERVERPROPERTY('ServerName') AS NVARCHAR(128)) AS SQLServerName,
CAST(SERVERPROPERTY('InstanceName') AS NVARCHAR(128)) AS InstanceName,
Case CAST(SERVERPROPERTY('IsClustered') AS VARCHAR(10))
     When 1 Then 'Yes' Else 'No' End AS IsClustered,
CAST(@Tcp_Port AS NVARCHAR(10)) AS TcpPort,
CAST(@sqlser AS NVARCHAR(50)) AS SQLServiceAccount,
Case CAST(SERVERPROPERTY('IsFullTextInstalled')  AS VARCHAR(10)) When 1 Then 'Yes' Else 'No' End AS IsFullTextInstalled, --1=enabled, 0=disbaled
Case CAST(SERVERPROPERTY('IsIntegratedSecurityOnly') AS NVARCHAR(10)) When 1 Then 'Yes' Else 'No' End AS IsIntegratedSecurityOnly, --1=integrated, 0=not integarted
Case CAST(SERVERPROPERTY('IsSingleUser') AS VARCHAR(10)) When 1 Then 'Yes' Else 'No' END AS IsSingleUser, --1=single user, 0=not single user
CAST(SERVERPROPERTY('LicenseType') AS NVARCHAR(20)) AS LicenseType , -- PER_SEAT = Per Seat mode, PER_PROCESSOR = Per-processor mode,DISABLED = Licensing is disabled
CAST(SERVERPROPERTY('MachineName') AS NVARCHAR(128)) AS MachineName,
CAST(SERVERPROPERTY('FilestreamShareName') AS NVARCHAR(255)) AS FilestreamShareName,
CAST(SERVERPROPERTY('ResourceLastUpdateDateTime') AS SMALLDATETIME) AS ResourceLastUpdateDateTime,
CAST(SERVERPROPERTY('ResourceVersion') AS NVARCHAR(128)) AS ResourceDBVersion,
CAST(SERVERPROPERTY('Collation') AS NVARCHAR(128)) AS Collation,
@cpu AS CPU_Count,
@ht AS Hyperthread_Ratio,
@mem_mb AS Physical_memory_MB,
